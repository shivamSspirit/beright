{
  "name": "beright-protocol",
  "version": "1.0.0",
  "description": "BeRight Protocol - Prediction market intelligence terminal built on OpenClaw",
  "main": "skills/telegramHandler.ts",
  "scripts": {
    "agent": "ts-node services/agentRunner.ts",
    "dev": "next dev -p 3001",
    "dev:full": "npx concurrently \"next dev -p 3001\" \"sleep 3 && ts-node skills/telegram.ts\"",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "skills:build": "tsc",
    "skills:start": "ts-node skills/telegramHandler.ts",
    "arb": "ts-node skills/arbitrage.ts",
    "research": "ts-node skills/research.ts",
    "whale": "ts-node skills/whale.ts",
    "markets": "ts-node skills/markets.ts",
    "brief": "ts-node skills/brief.ts",
    "heartbeat": "ts-node skills/heartbeat.ts loop",
    "heartbeat:once": "ts-node skills/heartbeat.ts once",
    "heartbeat:stats": "ts-node skills/heartbeat.ts stats",
    "telegram": "ts-node skills/telegram.ts",
    "telegram:status": "ts-node skills/telegram.ts --status",
    "telegram:unlock": "ts-node skills/telegram.ts --force-unlock",
    "test": "echo \"Running tests...\" && ts-node skills/markets.ts search bitcoin",
    "smart": "ts-node skills/smartBuilder.ts loop",
    "smart:once": "ts-node skills/smartBuilder.ts once",
    "smart:status": "ts-node skills/smartBuilder.ts status",
    "builder": "ts-node skills/buildLoop.ts loop",
    "builder:once": "ts-node skills/buildLoop.ts once",
    "builder:status": "ts-node skills/buildLoop.ts status",
    "builder:tasks": "ts-node skills/buildLoop.ts tasks",
    "automation": "ts-node services/automation.ts daemon",
    "automation:once": "ts-node services/automation.ts once",
    "automation:status": "ts-node services/automation.ts status",
    "intelligence": "ts-node skills/intelligence.ts",
    "feedback": "ts-node skills/feedback.ts",
    "recommend": "ts-node skills/recommendations.ts",
    "compare": "ts-node skills/comparison.ts",
    "learnings": "ts-node skills/learnings.ts",
    "smartpredict": "ts-node skills/smartPredict.ts",
    "market-alerts": "ts-node services/marketAlerts.ts",
    "weekly-summary": "ts-node services/weeklySummary.ts",
    "notifications": "ts-node services/notificationDelivery.ts",
    "scanner": "ts-node services/autonomousScanner.ts once",
    "scanner:daemon": "ts-node services/autonomousScanner.ts daemon",
    "autopredict": "ts-node services/autoPredictionEngine.ts once",
    "autopredict:daemon": "ts-node services/autoPredictionEngine.ts start",
    "autopredict:status": "ts-node services/autoPredictionEngine.ts status",
    "calibration": "ts-node services/selfCalibration.ts run",
    "calibration:daemon": "ts-node services/selfCalibration.ts daemon",
    "orchestrator": "ts-node services/autonomousOrchestrator.ts start",
    "orchestrator:status": "ts-node services/autonomousOrchestrator.ts status",
    "orchestrator:report": "ts-node services/autonomousOrchestrator.ts report",
    "portfolio": "ts-node services/portfolioManager.ts summary",
    "portfolio:positions": "ts-node services/portfolioManager.ts positions",
    "portfolio:exits": "ts-node services/portfolioManager.ts exits",
    "report:daily": "ts-node services/autonomousReporting.ts daily",
    "report:weekly": "ts-node services/autonomousReporting.ts weekly",
    "report:dashboard": "ts-node services/autonomousReporting.ts dashboard",
    "autonomous": "ts-node services/autonomousOrchestrator.ts start",
    "dev:frontend": "ts-node skills/devFrontend.ts",
    "dev:backend": "ts-node skills/devBackend.ts",
    "dev:test": "ts-node skills/devTest.ts",
    "validate": "npm run typecheck && npm run lint"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "@solana/web3.js": "^1.95.0",
    "@supabase/supabase-js": "^2.95.3",
    "@tavily/core": "^0.7.1",
    "bs58": "^5.0.0",
    "dotenv": "^16.4.0",
    "next": "^14.2.0",
    "node-fetch": "^3.3.0",
    "pino": "^9.0.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "xml2js": "^0.6.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/xml2js": "^0.4.14",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "prediction-markets",
    "polymarket",
    "kalshi",
    "arbitrage",
    "forecasting",
    "solana",
    "openclaw"
  ]
}
